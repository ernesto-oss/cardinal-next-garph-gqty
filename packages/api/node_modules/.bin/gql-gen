#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/cli/cjs/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/cli/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/cli/cjs/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/cli/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/cli/cjs/bin.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@graphql-codegen+cli@3.3.1_@babel+core@7.22.5_@types+node@20.3.1_graphql@16.6.0/node_modules/@graphql-codegen/cli/cjs/bin.js" "$@"
fi
