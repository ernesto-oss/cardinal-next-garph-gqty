#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/chokidar-cli@3.0.0/node_modules/chokidar-cli/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/chokidar-cli@3.0.0/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/chokidar-cli@3.0.0/node_modules/chokidar-cli/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/chokidar-cli@3.0.0/node_modules:/home/ernesto/Projects/oss/cardinal-next-garph-gqty/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/chokidar-cli@3.0.0/node_modules/chokidar-cli/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/chokidar-cli@3.0.0/node_modules/chokidar-cli/index.js" "$@"
fi
