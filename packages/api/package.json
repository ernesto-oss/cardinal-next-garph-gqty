{
  "name": "@acme/api",
  "version": "1.0.0",
  "main": "index.js",
  "types": "index.js",
  "scripts": {
    "codegen": "pnpm codegen:schema && pnpm codegen:client",
    "codegen:client": "npx @genql/cli --output ./genql --schema ./schema.graphql --esm",
    "codegen:schema": "graphql-codegen --config codegen.ts",
    "dev": "chokidar \"**/*.ts\" --silent --initial -i \"node_modules\" -i \"genql\" -c \"pnpm codegen\"",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env"
  },
  "dependencies": {
    "@acme/auth": "workspace:*",
    "@acme/database": "workspace:*",
    "@garph/gqty": "^0.4.5",
    "@pothos/core": "^3.30.0",
    "@pothos/plugin-scope-auth": "^3.19.2",
    "gqty": "3.0.0-alpha-e9f8f97b.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.9.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@genql/cli": "3.0.5",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/schema-ast": "^3.0.1",
    "@types/react": "^18.2.12",
    "chokidar-cli": "^3.0.0",
    "garph": "^0.5.7",
    "next": "13.4.4"
  }
}
